---
import { Image } from 'astro:assets';
import logo from '../assets/images/logo.svg'
import { SITE } from '~/utils/config';
---

<span class="self-center ml-2 rtl:ml-0 rtl:mr-2 text-2xl md:text-xl font-bold text-gray-900 whitespace-nowrap dark:text-white flex">
  <Image src={logo} alt="HackUDC logo" width={35} height={35} class="mr-2" /> {SITE?.name}
</span>
